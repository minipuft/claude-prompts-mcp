{
  "server": {
    "name": "Claude Custom Prompts",
    "version": "1.3.0",
    "port": 9090
  },
  "prompts": {
    "file": "prompts/promptsConfig.json"
  },
  "analysis": {
    "semanticAnalysis": {
      "enabled": false,
      "llmIntegration": {
        "enabled": false,
        "apiKey": null,
        "endpoint": null,
        "model": "gpt-4",
        "maxTokens": 1000,
        "temperature": 0.1
      }
    }
  },
  "transports": {
    "default": "stdio"
  },
  "logging": {
    "directory": "./logs",
    "level": "info"
  }
}
